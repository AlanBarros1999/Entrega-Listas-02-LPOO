Algoritmo "lista 2 questao 1"


Var
   num1:inteiro


Inicio
   escreval ("digite um numero")
   leia (num1)
   se (num1%2)=0 entao
   escreval ("numero par")
   senao
   escreval ("numero impar")
   fimse
   se (num1>=0) entao
   escreval ("numero positivo")
   senao
   escreval("numero negativo")
   fimse


Fimalgoritmo